<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    
    <style name="Mimictheme" parent="@style/Theme.Sherlock.Light">
        <item name="android:actionBarSize">55dp</item>
        <item name="actionBarSize">55dp</item>
        <item name="android:actionBarStyle">@style/Mimictheme.ActionBarStyle</item>
<item name="android:windowAnimationStyle">@null</item>
<item name="actionBarStyle">@style/Mimictheme.ActionBarStyle</item>
</style>

<style name="Mimictheme.ActionBarStyle" parent="Widget.Sherlock.Light.ActionBar">
<item name="android:titleTextStyle">@style/Mimictheme.ActionBar.TitleTextStyle</item>
<item name="titleTextStyle">@style/Mimictheme.ActionBar.TitleTextStyle</item>
</style>

<style name="Mimictheme.ActionBar.TitleTextStyle" parent="TextAppearance.Sherlock.Widget.ActionBar.Title">
<item name="android:textColor">@color/white</item>
<item name="android:textSize">25dp</item>
<item name="android:typeface">sans</item>

</style>

<style name="dialogbox" parent="@android:style/Theme.Holo.Dialog.NoActionBar">
   
</style>


</resources>
